{
    "name": "hoaaah/start-your-up",
    "description": "Template for Corporate Landing Page Site",
    "keywords": [
        "hoaaah",
        "corporate",
        "yii2",
        "landing"
    ],
    "homepage": "http://www.belajararief.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/hoaaah/corporate-site/issues?state=open",
        "forum": "http://www.belajararief.com/forum/",
        "wiki": "http://www.belajararief.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/hoaaah/corporate-site"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "2amigos/yii2-google-maps-library": "*",
        "kolyunya/yii2-map-input-widget": "~1.1",
        "2amigos/yii2-ckeditor-widget": "^2.0",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-detail-view": "*",
        "bizley/quill": "^2.0",
        "hoaaah/yii2-startbootstrap-agency": "dev-master",
        "kartik-v/yii2-widgets": "*",
        "sjaakp/yii2-symbol-picker": "dev-master",
        "2amigos/yii2-multi-select-widget": "*",
        "kartik-v/yii2-tabs-x": "*",
        "bizley/podium": "^0.7.0",
        "bizley/migration": "3.1",
        "nterms/yii2-mailqueue": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "phpunit/phpunit": "~9.5.0",
        "codeception/codeception": "^5.0.0 || ^4.0",
        "codeception/lib-innerbrowser": "^4.0 || ^3.0 || ^1.1",
        "codeception/module-asserts": "^3.0 || ^1.1",
        "codeception/module-yii2": "^1.1",
        "codeception/module-filesystem": "^3.0 || ^2.0 || ^1.1",
        "codeception/verify": "^3.0 || ^2.2",
        "symfony/browser-kit": "^6.0 || >=2.7 <=4.2.4",
        "c006/yii2-migration-utility": "dev-master"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}